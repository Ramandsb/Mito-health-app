<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context="in.tagbin.mitohealthapp.Fragments.Settings_frag">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp"
            android:layout_marginBottom="40dp">

            <TextView
                android:id="@+id/tvCustomNotification"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Custom Reminders"
                android:textColor="@color/colorPrimary"
                android:textAllCaps="true"
                android:textStyle="bold" />

            <View
                android:id="@+id/viewSperator"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvCustomNotification"
                android:layout_marginTop="10dp"
                android:background="#dedede" />

            <Switch
                android:id="@+id/switchFilterMeals"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:drawableLeft="@drawable/setting_meals"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Meals"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterMeals"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <Switch
                android:id="@+id/switchFilterWater"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator1"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:checked="true"
                android:drawableLeft="@drawable/setting_water"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Water"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterWater"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <Switch
                android:id="@+id/switchFilterSleep"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator2"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:drawableLeft="@drawable/setting_sleep"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Sleep"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterSleep"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <Switch
                android:id="@+id/switchFilterExercise"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator3"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:drawableLeft="@drawable/setting_exercise"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Exercise"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterExercise"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <Switch
                android:id="@+id/switchFilterWeeklyProgress"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator4"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:drawableLeft="@drawable/setting_weeklyprogress"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Weekly Progress"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterWeeklyProgress"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <Switch
                android:id="@+id/switchFilterHealthTips"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator5"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:drawableLeft="@drawable/setting_health"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Daily Health Tips"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterHealthTips"
                android:background="#dedede" />

            <TextView
                android:id="@+id/tvPeopleConnect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator6"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="7dp"
                android:text="People Connect"
                android:textColor="@color/colorPrimary"
                android:textAllCaps="true"
                android:textStyle="bold" />

            <View
                android:id="@+id/viewSperator7"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvPeopleConnect"
                android:layout_marginTop="7dp"
                android:background="#dedede" />

            <RelativeLayout
                android:id="@+id/relativeSettingGender"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator7"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingBottom="7dp"
                android:paddingTop="7dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Preference"
                    android:textColor="@color/colorPrimary"
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="Only Women    >"
                    android:id="@+id/tvSettingPreference"
                    android:textColor="@color/colorAccent"
                    android:textSize="15sp" />
            </RelativeLayout>

            <View
                android:id="@+id/viewSperator8"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/relativeSettingGender"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <RelativeLayout
                android:id="@+id/relativeSettingAge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator8"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingBottom="7dp"
                android:paddingTop="7dp">

                <TextView
                    android:id="@+id/tvSettingAge"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Age Range"
                    android:textColor="@color/colorPrimary"
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="5-80 yrs"
                    android:id="@+id/tvSettingAgeSet"
                    android:textColor="@color/colorAccent"
                    android:textSize="15sp" />

                <com.appyvet.rangebar.RangeBar xmlns:custom="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/rangebarAge"
                    android:layout_width="match_parent"
                    android:layout_height="72dp"
                    android:layout_below="@+id/tvSettingAge"
                    custom:connectingLineColor="@color/colorPrimary"
                    custom:pinColor="@color/colorPrimary"
                    custom:pinMaxFont="20sp"
                    custom:pinMinFont="14sp"
                    custom:pinPadding="10dp"
                    custom:pinRadius="17dp"
                    custom:rangeBarColor="#d0cfcf"
                    custom:selectorColor="@color/colorPrimary"
                    custom:textColor="#ffffff"
                    custom:tickColor="#000000"
                    custom:tickEnd="80"
                    custom:tickInterval="1"
                    custom:tickStart="5" />
            </RelativeLayout>

            <View
                android:id="@+id/viewSperator9"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/relativeSettingAge"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <RelativeLayout
                android:id="@+id/relativeSettingDistance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator9"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="12dp"
                android:paddingBottom="7dp"
                android:paddingTop="7dp">

                <TextView
                    android:id="@+id/tvSettingDistance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Maximum Distance"
                    android:textColor="@color/colorPrimary"
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="2 kms"
                    android:id="@+id/tvSettingDistanceSet"
                    android:textColor="@color/colorAccent"
                    android:textSize="15sp" />

                <org.adw.library.widgets.discreteseekbar.DiscreteSeekBar
                    android:id="@+id/rangebarDistance"
                    android:layout_width="match_parent"
                    android:layout_height="72dp"
                    android:layout_below="@+id/tvSettingDistance"
                    app:dsb_allowTrackClickToDrag="true"
                    app:dsb_min="2"
                    app:dsb_max="100"
                    app:dsb_indicatorColor="@color/colorPrimary"
                    app:dsb_indicatorFormatter="%d"
                    app:dsb_indicatorTextAppearance="@style/CustomFloaterTextAppearance"
                    app:dsb_progressColor="@color/colorPrimary" />
            </RelativeLayout>

            <View
                android:id="@+id/viewSperator10"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/relativeSettingDistance"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />

            <Switch
                android:id="@+id/switchFilterExplore"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator10"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Show me on Explore"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator11"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterExplore"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />
            <Switch
                android:id="@+id/switchFilterChatNotifications"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator11"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:drawableRight="@drawable/switch_thumb"
                android:text="Chat Notifications"
                android:textColor="@color/colorPrimary"
                android:textOff=""
                android:textOn=""
                android:textSize="15sp"
                android:thumb="@android:color/transparent"
                android:track="@android:color/transparent" />

            <View
                android:id="@+id/viewSperator12"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/switchFilterChatNotifications"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />
            <TextView
                android:id="@+id/tvSettingContactUs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Contact Us"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/viewSperator12"
                android:textColor="@color/colorPrimary"
                android:textSize="15sp" />
            <View
                android:id="@+id/viewSperator13"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvSettingContactUs"
                android:background="#dedede" />
            <TextView
                android:id="@+id/tvHelpSupport"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator13"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="7dp"
                android:text="Help &amp; Support"
                android:textColor="@color/colorPrimary"
                android:layout_centerHorizontal="true"
                android:textStyle="bold" />
            <View
                android:id="@+id/viewSperator14"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvHelpSupport"
                android:layout_marginTop="10dp"
                android:background="#dedede" />
            <TextView
                android:id="@+id/tvSettingPrivacy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Privacy Policy"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/viewSperator14"
                android:textColor="@color/colorPrimary"
                android:textSize="15sp" />
            <View
                android:id="@+id/viewSperator15"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvSettingPrivacy"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />
            <TextView
                android:id="@+id/tvSettingTerms"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Terms &amp; Conditions"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/viewSperator15"
                android:textColor="@color/colorPrimary"
                android:textSize="15sp" />
            <View
                android:id="@+id/viewSperator16"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvSettingTerms"
                android:layout_marginLeft="12dp"
                android:layout_marginRight="12dp"
                android:background="#dedede" />
            <TextView
                android:id="@+id/tvSettingFaqs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="FAQs"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="7dp"
                android:paddingBottom="10dp"
                android:layout_below="@+id/viewSperator16"
                android:textColor="@color/colorPrimary"
                android:textSize="15sp" />
            <View
                android:id="@+id/viewSperator17"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvSettingFaqs"
                android:background="#dedede" />
            <TextView
                android:id="@+id/tvLogout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/viewSperator17"
                android:paddingLeft="10dp"
                android:paddingTop="7dp"
                android:text="Log out"
                android:textColor="#ed273f"
                android:paddingBottom="10dp"
                android:textAlignment="center"
                android:layout_centerHorizontal="true"
                android:textStyle="bold" />
            <View
                android:id="@+id/viewSperator19"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/tvLogout"
                android:background="#dedede" />
            <pl.droidsonroids.gif.GifImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/progressBar1"
                android:layout_below="@+id/viewSperator19"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:indeterminateTintMode="src_atop"
                android:src="@drawable/splash_logo"
                />
            <TextView
                android:id="@+id/tvVersion"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/progressBar1"
                android:paddingLeft="10dp"
                android:paddingTop="7dp"
                android:text="Version 1.0"
                android:textColor="@color/colorPrimary"
                android:paddingBottom="10dp"
                android:textAlignment="center"
                android:layout_centerHorizontal="true"
                android:textStyle="bold" />
            <pl.droidsonroids.gif.GifImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/progressBar"
                android:layout_centerHorizontal="true"
                android:layout_centerInParent="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:layout_marginBottom="60dp"
                android:indeterminateTintMode="src_atop"
                android:visibility="gone"
                android:src="@drawable/loader"
                />
        </RelativeLayout>

